import java.util.Scanner;

public class Program {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();
        
        int b = a % 10;
        int c = a / 10 % 10;
        int d = a / 100 % 10;
        int f = a / 1000;
        int sum = b + c + d + f;
        System.out.println(sum);
        
    }
}
