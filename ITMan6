import java.util.Scanner;

public class Program {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();
        int b = a % 2;
        if (b == 0){
            System.out.println("false");
        }
        else {
            System.out.println("true");
        }
    }
}
