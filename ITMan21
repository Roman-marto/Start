import java.util.Scanner;

public class Program {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int x = scanner.nextInt();
        int i;
        while (x > 0) {
            i = scanner.nextInt();
            if (i < 0) {
                System.out.print(-i + " ");
            }
            else {
                System.out.print(i + " ");
            }
            x--;
        }
    }
}
